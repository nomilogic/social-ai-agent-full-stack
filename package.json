{
  "name": "social-agent-fullstack",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env  NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "dev:server": "NODE_ENV=development ts-node-dev --respawn --transpile-only --project tsconfig.server.json server/src/index.ts",
    "dev:client": "cd client && vite",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "cd client && vite build",
    "dev:server-only": "NODE_ENV=development ts-node-dev --respawn --transpile-only --project tsconfig.server.json server/src/index.ts",
    "dev:client-only": "cd client && vite --host",
    "type-check": "tsc --noEmit",
    "cleanup:storage": "tsx server/scripts/cleanup-storage.ts",
    "cleanup:dry-run": "tsx server/scripts/cleanup-storage.ts --dry-run --verbose",
    "cleanup:unreferenced": "tsx server/scripts/cleanup-storage.ts --unreferenced --dry-run --verbose",
    "cleanup:old": "tsx server/scripts/cleanup-storage.ts --days=30 --dry-run --verbose",
    "check:storage": "tsx server/scripts/check-storage-usage.ts",
    "diagnostic:storage": "tsx server/scripts/storage-diagnostic.ts",
    "analyze:storage": "tsx server/scripts/storage-analyzer.ts",
    "fix:rls": "tsx server/scripts/fix-rls-policies.ts",
    "test:upload": "tsx server/scripts/test-upload.ts",
    "test:database-rls": "tsx server/scripts/test-database-rls.ts",
    "check:file-limits": "tsx server/scripts/check-file-size-limits.ts",
    "test:real-upload": "tsx server/scripts/test-real-upload.ts",
    "type-check:server": "tsc --project tsconfig.server.json --noEmit",
    "generate-image": "node scripts/gemini-image-generation.js",
    "social-media-demo": "node scripts/social-media-image-demo.js",
    "gemini-enhanced": "node scripts/gemini-enhanced.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@google/genai": "^1.17.0",
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.55.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^1.2.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "express": "^4.21.2",
    "form-data": "^4.0.4",
    "http-proxy-middleware": "^2.0.9",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "PostCSS": "npm:postcss@^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.7.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
